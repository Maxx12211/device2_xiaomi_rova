typeattribute vendor_init data_between_core_and_vendor_violators;

allow vendor_init rootfs:file { getattr };
allow vendor_init media_rw_data_file:file { getattr };
allow vendor_init ipa_dev:file { create };
get_prop(vendor_init, vendor_imsradio_prop)

allow vendor_init {
  bluetooth_data_file
  camera_data_file
  media_rw_data_file
  nfc_data_file
  system_data_file
  wifi_data_file
  wpa_socket
  tombstone_data_file
  system_data_root_file
  ipa_dev
}:dir { create search getattr open read setattr ioctl write add_name remove_name rmdir relabelfrom };

allow vendor_init ipa_dev:file { rw_file_perms create };

allow vendor_init {
    proc_sysctl_autogroup
    proc_sysctl_schedboost
    proc_sched
    proc_dirty
    proc_dirty_ratio
}:file w_file_perms;

set_prop(vendor_init, vendor_power_prop)
get_prop(vendor_init, dirac_prop)
set_prop(vendor_init, dirac_prop)
set_prop(vendor_init, vendor_bluetooth_prop)
set_prop(vendor_init, camera_prop)
set_prop(vendor_init, vendor_bservice_prop)
set_prop(vendor_init, exported_default_prop)
set_prop(vendor_init, vendor_qcom_ims_prop)
set_prop(vendor_init, vendor_video_prop)
get_prop(vendor_init, vendor_xiaomi_prop)
